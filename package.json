    "react": "19.2.4",
    "react-dom": "19.2.4",
    "resend": "^6.9.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.4",
    "@tailwindcss/postcss": "^4.2.1",
    "@types/node": "^25.3.2",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "dotenv": "^17.3.1",
    "lefthook": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.1",
